		{
			"name": "new valid url bfs 200",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "adee9fd1-f577-483f-85b2-89403f6b9b27",
						"exec": [
							"pm.test(\"201 status code\", function () {",
							"    pm.response.to.have.status(201);",
							"});",
							"",
							"",
							"pm.test(\"content is valid\", function () {",
							"    let level2 = pm.response.json()[\"children\"];",
							"    let level3 = level2[0].children",
							"    ",
							"    pm.expect(pm.response.json()[\"children\"]).to.exist;",
							"    ",
							"    pm.expect(pm.response.json()[\"domainName\"]).to.eq(\"reddit\");",
							"    pm.expect(pm.response.json()[\"title\"]).to.eq(\"reddit: the front page of the internet\");",
							"    ",
							"    pm.expect(level2[0].children).to.exist;",
							"    pm.expect(level3[0].children).to.eq(null);",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
